{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "remix",
    "functions": {
        "app/entry.server.tsx": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/app/entry.server.tsx"
        }
    ]
}